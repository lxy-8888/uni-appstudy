.home {
  font-size: 14px;
  margin: 0px;
  padding: 0px;
  line-height: 24px;
}
.home .banner {
  /* 计算swiper的高度
swiper的宽度 / swiper的高度 = 原图的宽度/原图的高度
swiper的高度 = swiper的宽度*原图的高度 / 原图的宽度
*/
}
.home .banner swiper {
  height: calc(100vw * 352 / 1125);
}
.home .banner swiper image {
  width: 100%;
}
.home .category {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.home .category .type image {
  width: 190rpx;
}
.home .goodslist {
  margin-top: 20rpx;
}
.home .goodslist .list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  position: relative;
  height: 460rpx;
}
.home .goodslist .list .imagelist {
  width: 33%;
  height: 340rpx;
}
.home .goodslist .list .imagelist image {
  width: 100%;
}
.home .goodslist .list .imagelist:nth-child(2) {
  height: 120rpx;
}
.home .goodslist .list .imagelist:nth-child(2) image {
  height: 200rpx;
}
.home .goodslist .list .imagelist:nth-child(3) {
  height: 120rpx;
}
.home .goodslist .list .imagelist:nth-child(3) image {
  height: 200rpx;
}
.home .goodslist .list .imagelist:nth-child(4) {
  height: 120rpx;
  position: absolute;
  bottom: 130rpx;
  right: 250rpx;
}
.home .goodslist .list .imagelist:nth-child(4) image {
  height: 200rpx;
}
.home .goodslist .list .imagelist:nth-child(5) {
  position: absolute;
  bottom: 130rpx;
  right: 0rpx;
  height: 120rpx;
}
.home .goodslist .list .imagelist:nth-child(5) image {
  height: 200rpx;
}

